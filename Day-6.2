  static int findFibonacci(int n) {
        if (n <= 1) {
            return n;
        }

        int[] dp = new int[n + 1];
        dp[0] = 1;
        dp[1] = 1;

        for (int i = 2; i <= n; i++) {
            dp[i] = dp[i - 1] + dp[i - 2];
        }

        return dp[n];
    }
 

 // Space Optimization 

 static int findFibonacci(int n) {
        if (n <= 1) {
            return n;
        }

        
        prev = 1;
        last_previous= 1;

        for (int i = 2; i <= n; i++) {
            curr = prev + last_previous; 
            last_previous=prev;
            prev = curr;
        } 

        return prev;
    }
 