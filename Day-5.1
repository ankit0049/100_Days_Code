public  static  void call(int arr[] , int ind,int sum)
    {
        if(ind==arr.length)
        {
            return;
        }
       sum+=arr[ind];
        System.out.print(" "+sum);
        call(arr,ind+1,sum); 
        sum-=arr[ind];  
            System.out.print(" "+sum);
       call(arr,ind+1,sum);


    }
    public static void main(String[] args) {
        int arr[] ={1,3,4,2};
        call(arr,0,0);
    }